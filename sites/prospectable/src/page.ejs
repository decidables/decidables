---
toc:
  main:
    - {file: index, name: 'Introduction'}
    - {file: exploration, name: 'Model Exploration'}
    - {file: fitting, name: 'Model Fitting'}
    - {file: prediction, name: 'Model Predictions'}
  extra:
    - {file: references, name: 'References'}
    - {file: about, name: 'About'}
---
<!doctype html>
<html class="pro-html" lang="en">
  <head>
    <% for (var i=0; i < toc.main.length; i++) {
        if (toc.main[i].file == file) {
          var source = 'main';
          var index = i;
        }
      }
      for (var i=0; i < toc.extra.length; i++) {
        if (toc.extra[i].file == file) {
          var source = 'extra';
          var index = i;
        }
      }
      var windowTitle = toc[source][index].name.replace(/<[^>]+>/g, ''); %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="prospectable: <%= title %>">
    <meta name="author" content="Dr. Adam Krawitz">

    <title>prospectable: <%- windowTitle %></title>

    <link href="page.css" rel="stylesheet">
  </head>
  <body class="pro-body">
    <div class="pro-page">
      <div class="pro-layout">

        <div class="pro-sidebar">
          <div class="pro-nav-group">
            <div class="pro-nav-top">
              <button class="pro-nav-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#pro-nav" aria-controls="pro-nav" aria-expanded="false" aria-label="Toggle table of contents">
                <%- include(resolvePkg('bootstrap-icons/icons/list.svg')) %>
              </button>
              <a class="pro-nav-brand" href="index.html"><span class="prospectable">prospect<span class="math-var">α</span>b<span class="math-var">λ</span>e</span></a>
           </div>
            <nav class="pro-nav-collapse collapse" id="pro-nav">
              <ul class="pro-toc pro-toc-main">
                <% for (var i=0; i < toc.main.length; i++) { %>
                  <% if ((source == 'main') && (index == i)) { %>
                    <li class="pro-toc-item pro-toc-item-active">
                  <% } else { %>
                    <li class="pro-toc-item">
                  <% } %>
                      <a class="pro-toc-link" href="<%= toc.main[i].file %>.html">
                        <%- toc.main[i].name %>
                      </a>
                    </li>
                <% } %>
              </ul>
              <ul class="pro-toc pro-toc-extra">
                <% for (var i=0; i < toc.extra.length; i++) { %>
                  <% if ((source == 'extra') && (index == i)) { %>
                    <li class="pro-toc-item pro-toc-item-active">
                  <% } else { %>
                    <li class="pro-toc-item">
                  <% } %>
                      <a class="pro-toc-link" href="<%= toc.extra[i].file %>.html">
                        <%- toc.extra[i].name %>
                      </a>
                    </li>
                <% } %>
              </ul>
              <ul class="pro-toc pro-toc-external">
                <li class="pro-toc-item">
                  <span class="pro-toc-text">v<%- utilities.getPackageVersion() %></span>
                </li>
                <li class="pro-toc-item">
                  <a class="pro-toc-link" href="https://creativecommons.org/licenses/by-sa/4.0/" target="_blank" rel="noopener">CC-BY-SA-4.0</a>
                </li>
                <li class="pro-toc-item">
                  <a class="pro-toc-link" href="https://www.gnu.org/licenses/gpl-3.0.html" target="_blank" rel="noopener">GPL-3.0-or-later</a>
                </li>
                <li class="pro-toc-item">
                  <a class="pro-toc-link" href="https://github.com/decidables/decidables/tree/main/sites/prospectable" target="_blank" rel="noopener">
                    <%- include(resolvePkg('bootstrap-icons/icons/github.svg')) %>
                  </a>
                </li>
              </ul>
            </nav>
          </div>
          <div class="pro-nav-bottom">
            <a class="pro-nav-decidables" href="../"><span class="decidables">decidables</span></a>
          </div>
        </div>

        <main class="pro-main">
          <header>
            <h1><%- title %></h1>
          </header>
          <section class="pro-content">
            <%- contents %>
          </section>

          <footer class="pro-footer">
            <div class="pro-footer-item pro-footer-item-left">
              <% if ((source == 'main') && (index > 0)) { %>
                <a class="pro-toc-link" href="<%= toc.main[index - 1].file %>.html">&lt; <%- toc.main[index - 1].name %></a>
              <% } %>
            </div>
            <div class="pro-footer-item pro-footer-item-right">
              <% if ((source == 'main') && (index < (toc.main.length - 1))) { %>
                <a class="pro-toc-link" href="<%= toc.main[index + 1].file %>.html"><%- toc.main[index + 1].name %> &gt;</a>
              <% } %>
            </div>
          </footer>
        </main>

      </div>
    </div>
    <script type="module" src="<%= script %>.js"></script>
  </body>
</html>
