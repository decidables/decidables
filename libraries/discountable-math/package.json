{
  "name": "@decidables/discountable-math",
  "version": "0.1.2",
  "description": "discountable-math: Equations for calculating Hyperbolic Temporal Discounting",
  "keywords": [
    "explorable explanation",
    "explorable",
    "decision making",
    "decidables",
    "hyperbolic temporal discounting",
    "temporal discounting",
    "delay discounting",
    "discountable"
  ],
  "type": "module",
  "main": "./lib/discountableMath.umd.min.js",
  "browser": "./lib/discountableMath.umd.min.js",
  "module": "./lib/discountableMath.esm.min.js",
  "exports": {
    "./lib/*.js": "./lib/*.js",
    ".": "./src/index.js",
    "./*": "./src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decidables/decidables.git",
    "directory": "libraries/discountable-math"
  },
  "bugs": "https://github.com/decidables/decidables/issues",
  "homepage": "https://github.com/decidables/decidables/tree/main/libraries/discountable-math",
  "author": "Adam Krawitz (https://web.uvic.ca/psyc/krawitz/)",
  "license": "CC-BY-SA-4.0 AND GPL-3.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package.json",
    "*.md",
    "src",
    "lib"
  ],
  "scripts": {
    "lint": "gulp lint",
    "test": "web-test-runner test/**/*.test.js --config ../../.webtestrunnerrc.js",
    "test:watch": "web-test-runner test/**/*.test.js --config ../../.webtestrunnerrc.js --watch",
    "test:file": "web-test-runner $@ --config ../../.webtestrunnerrc.js",
    "build": "gulp build"
  },
  "devDependencies": {
    "@web/test-runner": "^0.18.0",
    "gulp": "^4.0.2"
  }
}
