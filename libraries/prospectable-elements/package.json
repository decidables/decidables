{
  "name": "@decidables/prospectable-elements",
  "version": "0.2.6",
  "description": "prospectable-elements: Web Components for visualizing Cumulative Prospect Theory",
  "keywords": [
    "web component",
    "explorable explanation",
    "explorable",
    "decision making",
    "decidables",
    "cumulative prospect theory",
    "value",
    "subjective value",
    "probability",
    "decision weight",
    "utility",
    "subjective utility",
    "prospectable"
  ],
  "type": "module",
  "main": "./lib/prospectableElements.umd.min.js",
  "browser": "./lib/prospectableElements.umd.min.js",
  "module": "./lib/prospectableElements.esm.min.js",
  "exports": {
    "./lib/*.js": "./lib/*.js",
    ".": "./src/index.js",
    "./components": "./src/components/index.js",
    "./equations": "./src/equations/index.js",
    "./examples": "./src/examples/index.js",
    "./*": "./src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decidables/decidables.git",
    "directory": "libraries/prospectable-elements"
  },
  "bugs": "https://github.com/decidables/decidables/issues",
  "homepage": "https://github.com/decidables/decidables/tree/main/libraries/prospectable-elements",
  "author": "Adam Krawitz (https://web.uvic.ca/psyc/krawitz/)",
  "license": "CC-BY-SA-4.0 AND GPL-3.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package.json",
    "*.md",
    "src",
    "lib"
  ],
  "scripts": {
    "lint": "gulp lint",
    "test": "web-test-runner test/**/*.test.js --config ../../.webtestrunnerrc.js",
    "test:watch": "web-test-runner test/**/*.test.js --config ../../.webtestrunnerrc.js --watch",
    "test:file": "web-test-runner $@ --config ../../.webtestrunnerrc.js",
    "build": "gulp build"
  },
  "devDependencies": {
    "@web/test-runner": "^0.18.0",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.1.0",
    "gulp-inject": "^5.0.5",
    "gulp-rename": "^2.0.0",
    "node-sass-package-importer": "^5.3.3"
  },
  "dependencies": {
    "@decidables/decidables-elements": "^0.4.0",
    "@decidables/prospectable-math": "^0.2.3",
    "@lit-labs/motion": "^1.0.6",
    "bayes.js": "https://github.com/akrawitz/bayes.js.git#commit=c7b091b75f85a86b6a3965a983b31e23d9ef456f",
    "d3": "^7.8.5",
    "jstat": "^1.9.6",
    "lit": "^3.1.0",
    "plotly.js": "^2.27.1",
    "regenerator-runtime": "^0.14.0"
  }
}
