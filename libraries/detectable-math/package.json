{
  "name": "@decidables/detectable-math",
  "version": "0.1.7",
  "description": "detectable-math: Equations for calculating Signal Detection Theory",
  "keywords": [
    "explorable explanation",
    "explorable",
    "decision making",
    "decidables",
    "signal detection theory",
    "sensitivity",
    "response bias",
    "hit",
    "miss",
    "false alarm",
    "correct rejection",
    "hit rate",
    "false alarm rate",
    "accuracy",
    "detectable"
  ],
  "type": "module",
  "main": "./lib/detectableMath.umd.min.js",
  "browser": "./lib/detectableMath.umd.min.js",
  "module": "./lib/detectableMath.esm.min.js",
  "exports": {
    "./lib/*.js": "./lib/*.js",
    ".": "./src/index.js",
    "./*": "./src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decidables/decidables.git",
    "directory": "libraries/decidables-math"
  },
  "bugs": "https://github.com/decidables/decidables/issues",
  "homepage": "https://github.com/decidables/decidables/tree/main/libraries/detectable-math",
  "author": "Adam Krawitz (https://web.uvic.ca/psyc/krawitz/)",
  "license": "CC-BY-SA-4.0 AND GPL-3.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package.json",
    "*.md",
    "src",
    "lib"
  ],
  "scripts": {
    "lint": "gulp lint",
    "test": "web-test-runner test/**/*.test.js --config ../../.webtestrunnerrc.js",
    "test:watch": "web-test-runner test/**/*.test.js --config ../../.webtestrunnerrc.js --watch",
    "test:file": "web-test-runner $@ --config ../../.webtestrunnerrc.js",
    "build": "gulp build"
  },
  "devDependencies": {
    "@web/test-runner": "^0.18.0",
    "gulp": "^4.0.2"
  },
  "dependencies": {
    "jstat": "^1.9.6"
  }
}
